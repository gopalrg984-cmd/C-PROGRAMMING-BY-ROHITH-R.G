#include<stdio.h>
void swap(float*a,float*b){
    float temp=*a;
    *a=*b;
    *b=temp;
} 
    int main(){
        float x,y;
        printf("enter two balnces; ");
        scanf("%f%f",&x,&y);
        
        printf("before swap: %.2f %f/n",x,y);
        swap(&x,&y);
        printf("After swap:  %.2f %f/n",x,y);
    }
