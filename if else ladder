#include <stdio.h>

int main() {
    int marks;
    
    printf("Enter the value of marks: ");
    scanf("%d", &marks);

    if (marks > 90) {
        printf("Distinction\n");
    } 
    else if (marks > 70) {
        printf("Good\n");
    } 
    else if (marks > 50) {
        printf("Should get better marks\n");
    } 
    else {
        printf("Fail\n");
    }

    return 0;
}
