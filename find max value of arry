#include<stdio.h>
int main(){
    int a[5],n,i,max;
    printf("enter the number of elements ");
    scanf("%d",&n);
    printf(" enter %d elements:\n",n);
    for(i=0; i<n; i++);
    scanf("%d",&a[i]);

    max;a,0;
     for(i = 0; i < n; i++) {
        if(a[i] > max) {
            max = a[i];
        }
    }   
     printf("largest element in the arry is:%d\n",max);
}  
