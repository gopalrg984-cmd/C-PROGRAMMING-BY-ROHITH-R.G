#include <stdio.h>
#include <math.h>

int main() {
    float a, b, c, d;

    printf("Enter coefficients a, b, c: ");
    scanf("%f %f %f", &a, &b, &c);

    d = (b*b) - (4*a*c);

    if(d > 0)
        printf("Real and Distinct Roots");
    else if(d == 0)
        printf("Real and Equal Roots");
    else
        printf("Imaginary Roots");

    return 0;
}
